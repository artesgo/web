{
  "name": "fdn",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:fdn": "ng serve --project=fdn --port=4202",
    "start:grw": "ng serve --project=growcery --port=4201",
    "prebuild:fdn": "rm -r ./dist/apps/ && rsync -r ./config/fdn/ ./dist/apps/",
    "prebuild:grw": "rm -r ./dist/apps/ && rsync -r ./config/growcery/ ./dist/apps/",
    "build:fdn": "ng build fdn",
    "build:grw": "ng build growcery",
    "build:lib": "ng build --project=foundation && cp ./libs/foundation/_fdn.scss ./dist/libs/foundation/ && rsync -r ./libs/foundation/styles/* ./dist/libs/foundation/styles",
    "test:fdn": "ng test --project=fdn -- --watch",
    "test:grw": "ng test --project=growcery -- --watch",
    "test:lib": "ng test --project=foundation -- --watch",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ngneat/transloco": "^2.5.2",
    "@ngrx/effects": "^8.4.0",
    "@ngrx/store": "^8.4.0",
    "@ngrx/store-devtools": "^8.4.0",
    "@types/stripe-v3": "^3.1.9",
    "artesgo-foundation": "0.0.4",
    "core-js": "^2.6.10",
    "d3": "^5.12.0",
    "firebase": "^6.6.2",
    "hammerjs": "^2.0.8",
    "rxjs": "6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular-devkit/build-ng-packagr": "^0.13.9",
    "@angular/cli": "7.3.1",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@nrwl/builders": "7.8.3",
    "@nrwl/jest": "8.6.0",
    "@nrwl/schematics": "7.8.3",
    "@nrwl/workspace": "^8.7.0",
    "@types/d3": "^5.7.2",
    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cypress": "^3.6.0",
    "dotenv": "6.2.0",
    "jest": "24.1.0",
    "jest-docblock": "^24.9.0",
    "jest-marbles": "^2.5.0",
    "jest-preset-angular": "^7.1.1",
    "ng-packagr": "^10.1.1",
    "prettier": "1.16.4",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "3.2.2"
  }
}
